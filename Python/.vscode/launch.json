{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "随机用户名生成",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/account/username/main.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Python-调试当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "SummerPython-图片转ICO",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/picture/to_ico/all_in_one.py",
      "console": "integratedTerminal",
      "args": [
        "${workspaceFolder}/use_case/picture/to_ico/input/logo_transparent.png",
        "${workspaceFolder}/use_case/picture/to_ico/out/logo_transparent.ico"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 运行所有功能演示",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 当前毫秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--current"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 当前秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--current", "--unit", "seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 指定时间的毫秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--datetime", "2026-01-08 12:30:45"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 指定时间的秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--datetime", "2026-01-08 12:30:45", "--unit", "seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 指定日期零点毫秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--date", "2025-01-08"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 指定日期零点秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--date", "2025-01-08", "--unit", "seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 今天零点毫秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--today"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 今天零点秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--today", "--unit", "seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 明天零点毫秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--tomorrow"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 明天零点秒级时间戳",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--tomorrow", "--unit", "seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 毫秒转秒级",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--convert", "1736330444000 milliseconds seconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "时间戳 - 秒级转毫秒",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/use_case/time/timestamp.py",
      "console": "integratedTerminal",
      "args": ["--convert", "1736330444 seconds milliseconds"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
